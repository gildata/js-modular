{
    "manifest_version": 2,
    "name": "JSON Format Plugin",
    "description": "This extension will format a JSON file using JFP!",
    "version": "1.0",
    "browser_action": {
        "default_icon": "chrome-extensions-icon.png",
        "default_popup": "app.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*",
        "storage"
    ]
}